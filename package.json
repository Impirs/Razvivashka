{
  "name": "summ_solver",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.impirs.razvivashka",
    "productName": "Razvivashka",
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "files": [
      "app.js",
      "preload.js",
      "renderer.js",
      "data_manager.js",
      "index.html",
      "style_base.css",
      "style_overlap_module.css",

      "page_catalog.html",
      "page_catalog.css",
      "page_achievements.html",
      "page_achievements.js",
      "page_achievements.css",
      "page_settings.html",
      "page_settings.js",
      "page_settings.css",

      "game_digit_solver.html",
      "game_digit.js",
      "game_digit.css",

      "assets/**/*"
    ],
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Impirs/Summ_solver.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Impirs/Summ_solver/issues"
  },
  "homepage": "https://github.com/Impirs/Summ_solver#readme",
  "devDependencies": {
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8"
  }
}
